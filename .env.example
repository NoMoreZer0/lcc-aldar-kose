# MinIO admin credentials used by docker-compose and the ML service
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=change-me

# S3 credentials the ML service uses for presigned URLs (can match MinIO root user)
AWS_ACCESS_KEY_ID=admin
AWS_SECRET_ACCESS_KEY=change-me

# Endpoint where MinIO (or another S3-compatible service) is reachable
AWS_ENDPOINT_URL=http://localhost:9000

# OpenAI key for storyboard planning prompts
OPENAI_API_KEY=sk-your-openai-key

# URL of the ML GPU microservice as seen from the backend
GPU_SERVICE_URL=http://host.docker.internal:8081
